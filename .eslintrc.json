{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "fetch": true,
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["chai-friendly"],
  "rules": {
    "no-console": "off",
    "no-unused-expressions": 0,
    "chai-friendly/no-unused-expressions": 2,
    "object-curly-newline": "off"
  }
}
